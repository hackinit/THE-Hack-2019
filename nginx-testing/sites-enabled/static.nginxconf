server {
    listen 80;
    listen [::]:80;
    server_name static.thehack.org.cn;

    location /2019 {
        # The static files for year 2018
        expires -1;
        alias /usr/share/static/2019/;
    }

    location /2018 {
        # The static files for year 2018
        expires -1;
        alias /usr/share/static/2018/;
    }
}
